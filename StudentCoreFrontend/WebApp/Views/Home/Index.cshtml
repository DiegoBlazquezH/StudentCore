@{
    ViewData["Title"] = "Home Page";
}

@model IEnumerable<Common.Logic.Model.Student>

<div class="container-fluid">
    <h1>Cliente ASP.Net Core</h1>
    <table class="table">
        <tr>
            <th>GUID</th>
            <th>ID</th>
            <th>Name</th>
            <th>Surname</th>
            <th>DNI</th>
            <th>BirthDate</th>
            <th>Age</th>
            <th>RegisterDate</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.GUID</td>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.DNI</td>
            <td>@item.BirthDate</td>
            <td>@item.Age</td>
            <td>@item.RegisterDate</td>
        </tr>
        }
    </table>
</div>